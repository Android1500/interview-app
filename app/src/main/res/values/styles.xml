<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextAppearance.AppCompat.Large.InterviewApp" parent="TextAppearance.AppCompat.Large">
        <item name="android:fontFamily">@font/google_sans_text_medium</item>
    </style>

    <style name="TextAppearance.AppCompat.ExtraLarge.InterviewApp" parent="TextAppearance.AppCompat.Large">
        <item name="android:fontFamily">@font/google_sans_text_medium</item>
        <item name="android:textSize">32sp</item>
    </style>

    <style name="TextAppearance.AppCompat.Medium.InterviewApp" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/google_sans_text_medium</item>
    </style>

    <style name="TextAppearance.AppCompat.Small.InterviewApp" parent="TextAppearance.AppCompat.Small">
        <item name="android:fontFamily">@font/google_sans_text_regular</item>
    </style>

    <style name="TextAppearance.AppCompat.Small.InterviewApp.Medium" parent="TextAppearance.AppCompat.Small">
        <item name="android:fontFamily">@font/google_sans_text_medium</item>
